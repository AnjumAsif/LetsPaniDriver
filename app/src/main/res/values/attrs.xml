<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontStyle">
        <attr name="fontName" format="string"/>
        <attr name="boldornot" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="RotateLoading">
        <attr name="loading_width" format="dimension"/>
        <attr name="loading_color" format="color"/>
        <attr name="shadow_position" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TwitterSplashView">
        <attr name="icon" format="reference"/>
        <attr name="iconColor" format="color"/>
        <attr name="holeFillColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="removeFromParentOnEnd" format="boolean"/>
    </declare-styleable>

</resources>